= styles_for :user

.title_tools
  / = link_to_create(@user, new_admin_user_path, :text => t(:create_user))
  / |
  = link_to_invite(@user, new_user_invitation_path, :text => t(:invite_user))

.title
  %span#create_user_title #{t :users}
  = image_tag("loading2.gif", :size => "16x16", :id => "loading", :style => "display: none;")

.list#users= render :partial => "user", :collection => @users
#paginate= render :partial => "shared/paginate"